<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(accomodation) -%>">
  <span class='title'>
    <%= accomodation.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.accomodations_accomodation_path(accomodation),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_accomodations_admin_accomodation_path(accomodation),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.accomodations_admin_accomodation_path(accomodation),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => accomodation.name),
        :method => :delete %>
  </span>
</li>
