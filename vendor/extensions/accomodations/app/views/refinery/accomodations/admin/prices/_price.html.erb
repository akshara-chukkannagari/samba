<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(price) -%>">
  <span class='title'>
    <%= price.distribution %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_accomodations_admin_price_path(price),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.accomodations_admin_price_path(price),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => price.distribution),
        :method => :delete %>
  </span>
</li>
